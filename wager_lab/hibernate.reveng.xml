<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-name="IX_BIODATA"/>
  <table-filter match-name="IX_BIODATA_FIELD"/>
  <table-filter match-name="IX_BIODATA_TYPES"/>
  <table-filter match-name="IX_BIODATA_UNITS"/>
  
  <table name="IX_BIODATA" class="org.wager.lims.biodata.Data"/>
   <table name="IX_BIODATA_FIELD" class="org.wager.lims.biodata.Field"/>
   <table name="IX_BIODATA_TYPES"  class="org.wager.lims.biodata.Type"/>
    <table name="IX_BIODATA_UNITS"  class="org.wager.lims.biodata.Unit"/>
</hibernate-reverse-engineering>