<?xml version="1.0" encoding="UTF-8"?>
<content-types>
	<content-type type="text/xml" label="XML Document" icon="media/edu/columbia/ais/portal/doc_xml.gif">
        <equivalent type="application/xml"/>
		<file-extension value="xml" preferred="true"/>
        <editor key="dublincore"/>
		<editor key="xml"/>
        <editor key="buildoptions"/>
	    <editor key="upload"/>
    </content-type>
	<content-type type="application/xml+xslt" label="XSL Transformation" icon="media/edu/columbia/ais/portal/doc_xsl.gif">
		<file-extension value="xsl" preferred="true"/>
		<editor key="upload"/>
	</content-type>
    <content-type type="application/xml-dtd" label="XML DTD" icon="media/edu/columbia/ais/portal/doc_dtd.gif">
        <file-extension value="dtd"/>
        <editor key="text"/>
        <editor key="upload"/>
    </content-type>
    <content-type type="text/html" label="HTML Document" icon="media/edu/columbia/ais/portal/doc_html.gif">
		<file-extension value="html" preferred="true"/> 
                <file-extension value="htm"/> 
		<editor key="text"/>
	</content-type>
	<content-type type="text/plain" label="Text Document" icon="media/edu/columbia/ais/portal/doc_blank.gif">
		<file-extension value="txt" preferred="true"/>
		<editor key="text"/>
	</content-type>
	<content-type type="text/javascript" label="Javascript" icon="media/edu/columbia/ais/portal/doc_blank.gif">
		<file-extension value="js"/>
                <editor key="text"/>
	</content-type>
	<content-type type="text/css" label="Cascading Stylesheet" icon="media/edu/columbia/ais/portal/doc_blank.gif">
		<file-extension value="css"/>
                <editor key="text"/>
	</content-type>
    <content-type type="text/directory" label="Contact Info" icon="media/edu/columbia/ais/portal/doc_blank.gif">
		<file-extension value="vcf"/>
                <editor key="text"/>
	</content-type>
	<content-type type="application/pdf" label="PDF Document" icon="media/edu/columbia/ais/portal/doc_blank.gif">
			<file-extension value="pdf"/>
        <editor key="upload"/>
	</content-type>
	<content-type type="application/msword" label="Microsoft Word Document" icon="media/edu/columbia/ais/portal/doc_blank.gif">
			<file-extension value="doc" preferred="true"/>
        <editor key="upload"/>
	</content-type>
	<content-type type="application/vnd.ms-powerpoint" label="Microsoft PowerPoint Document" icon="media/edu/columbia/ais/portal/doc_blank.gif">
			<equivalent type="application/powerpoint"/>
			<file-extension value="ppt"/>
        <editor key="upload"/>
	</content-type>
	<content-type type="application/vnd.ms-excel" label="Microsoft Excel Document" icon="media/edu/columbia/ais/portal/doc_blank.gif">
			<equivalent type="application/vnd.excel"/>
			<equivalent type="application/msexcel"/>
			<file-extension value="xls" preferred="true"/>
        <editor key="upload"/>
	</content-type>
	<content-type type="image/jpeg" label="JPEG Image" icon="media/edu/columbia/ais/portal/doc_jpg.gif">
                <equivalent type="image/pjpeg"/>
		<file-extension value="jpg" preferred="true"/>
                <file-extension value="jpeg"/>
                <file-extension value="jpe"/>
               <editor key="upload"/>
	        <editor key="dublincore"/>
        </content-type>
	<content-type type="image/gif" label="GIF Image" icon="media/edu/columbia/ais/portal/doc_gif.gif">
		<file-extension value="gif"/>
                <editor key="upload"/>
	        <editor key="dublincore"/>
    </content-type>
    <content-type type="application/zip" label="Zip File" icon="media/edu/columbia/ais/portal/doc_blank.gif">
		<equivalent type="application/x-zip-compressed"/>
		<file-extension value="zip" preferred="true"/>
        <editor key="upload"/>
	    <editor key="dublincore"/>
    </content-type>
    <content-type type="audio/mpeg" label="MP3" icon="media/edu/columbia/ais/portal/doc_blank.gif">
		<file-extension value="mp3" preferred="true"/>
        <editor key="upload"/>
	    <editor key="dublincore"/>
    </content-type>
    <content-type type="image/png" label="PNG Image" icon="media/edu/columbia/ais/portal/doc_blank.gif">
		<file-extension value="png" preferred="true"/>
        <editor key="upload"/>
	    <editor key="dublincore"/>
    </content-type>
    <content-type type="video/mpeg" label="MPEG Video" icon="media/edu/columbia/ais/portal/doc_blank.gif">
		<file-extension value="mpg" preferred="true"/>
		<file-extension value="mpe"/>
		<file-extension value="mpeg"/>
        <editor key="upload"/>
	    <editor key="dublincore"/>
    </content-type>
    <content-type type="video/x-msvideo" label="AVI Video" icon="media/edu/columbia/ais/portal/doc_blank.gif">
		<file-extension value="avi" preferred="true"/>
        <editor key="upload"/>
	    <editor key="dublincore"/>
    </content-type>
</content-types>
 