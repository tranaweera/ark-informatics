<dbschema type="postgres">

        <domain iname="ORGGROUPTREE" ename="ix_org_group" deletedfield="true" sequence="ix_org_group_seq">
                <field iname="intOrgGroupKey" ename="ORGGROUPKEY" lname="Org group ID" cname="Org Group ID" type="integer" length="8" unique="true" required="false" />
		<field iname="strOrgGroupName" ename="NAME" lname="Business unit name" cname="Business unit name" type="string" length="100" unique="false" required="true" />
		<field iname="intParentGroupKey" ename="PARENTKEY" lname="Org group ID" cname="Org Group ID" type="integer" length="8" unique="false" required="false" />
                <field iname="intDeleted" ename="DELETED" lname="Deleted" cname="Deleted" type="integer" length="8" unique="false" required="false" />
                <field iname="intGroupKey" ename="GROUPKEY" lname="Group ID" cname="Group ID" type="integer" length="8" unique="false" required="false" />
        
        
        </domain>

        <domain iname="ORGUSERGROUPTREE" ename="ix_org_user_group" deletedfield="true" sequence="ix_org_user_group_seq">
                <field iname="intOrgUserGroupKey" ename="ORGUSERGROUPKEY" lname="Org user group ID" cname="Org user group ID" type="integer" length="8" unique="true" required="false" />
		<field iname="intOrgGroupKey" ename="ORGGROUPKEY" lname="Org group ID" cname="Org Group ID" type="integer" length="8" unique="false" required="true" />
		<field iname="intOrgUserKey" ename="ORGUSERKEY" lname="Org user ID" cname="Org user ID" type="integer" length="8" unique="false" required="true" />
                <field iname="intDeleted" ename="DELETED" lname="Deleted" cname="Deleted" type="integer" length="8" unique="false" required="false" />
                <!--field iname="intGroupKey" ename="GROUPKEY" lname="Group ID" cname="Group ID" type="integer" length="8" unique="false" required="false" /-->
        
        
        </domain>

        <domain iname="ORGUSERTREE" ename="ix_org_user" deletedfield="true" sequence="ix_org_user_seq">
                <field iname="intOrgUserKey" ename="ORGUSERKEY" lname="Org user ID" cname="Org user ID" type="integer" length="8" unique="true" required="false" />
                <field iname="strFirstName" ename="FIRST_NAME" lname="First name" cname="First name" type="string" length="30" unique="false" required="true" />
                <field iname="strLastName" ename="LAST_NAME" lname="Last name" cname="Last name" type="string" length="30" unique="false" required="true" />
                <field iname="intDeleted" ename="DELETED" lname="Deleted" cname="Deleted" type="integer" length="8" unique="false" required="false" />
                <field iname="intGroupKey" ename="GROUPKEY" lname="Group ID" cname="Group ID" type="integer" length="8" unique="false" required="false" />
        
        
        </domain>

        <formfield name="orggrouptree_view">
            <fieldname name="ORGGROUPTREE_intOrgGroupKey"/>
            <fieldname name="ORGGROUPTREE_strOrgGroupName"/>
            <fieldname name="ORGGROUPTREE_intParentGroupKey"/>
            
        
        </formfield>
        
        <formfield name="orgusergrouptree_view">
            <fieldname name="ORGUSERGROUPTREE_intOrgUserGroupKey"/>
            <fieldname name="ORGUSERGROUPTREE_intOrgGroupKey"/>
            <fieldname name="ORGUSERGROUPTREE_intOrgUserKey"/>
            
            <!--fieldname name="ORGUSERGROUP_intGroupKey"-->
            
        </formfield>
        
        <formfield name="orgusertree_view">
            <fieldname name="ORGUSERTREE_intOrgUserKey"/>
            <fieldname name="ORGUSERTREE_strFirstName"/>
            <fieldname name="ORGUSERTREE_strLastName"/>
        
        </formfield>
            
        
        
        
  
  
</dbschema>
