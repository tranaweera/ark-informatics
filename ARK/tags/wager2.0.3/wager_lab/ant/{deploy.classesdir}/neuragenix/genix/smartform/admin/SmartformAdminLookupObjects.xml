<?xml version='1.0' encoding='utf-8'?>


<lookup_objects>

	<lookup_object name="Investigators" primaryfield="PATIENT_intPatientID">
		<domain name="PATIENT" />
		<domain name="BIOSPECIMEN" />

		<join type="INNER JOIN" first_field="PATIENT_intPatientID" second_field="BIOSPECIMEN_intPatientID" />

                <fieldname name="PATIENT_strSurname" dname="Surname" inwhere="true" infieldlist="true" />
                <fieldname name="PATIENT_strFirstName" dname="First name" inwhere="true" infieldlist="true" />
                <fieldname name="BIOSPECIMEN_strBiospecimenID" dname="Biospecimen ID" inwhere="true" infieldlist="true" />

                <displayfield name="PATIENT_strSurname" />
                <displayfield name="PATIENT_strFirstName" />

	</lookup_object>
        
        <lookup_object name="Case" primaryfield="PATIENT_intPatientID">
		<domain name="PATIENT" />
		<domain name="BIOSPECIMEN" />

		<join type="INNER JOIN" first_field="PATIENT_intPatientID" second_field="BIOSPECIMEN_intPatientID" />

                <fieldname name="PATIENT_strSurname" dname="Surname" inwhere="false" infieldlist="false" />
                <fieldname name="PATIENT_strFirstName" dname="First name" inwhere="false" infieldlist="false" />
                <fieldname name="BIOSPECIMEN_strBiospecimenID" dname="Biospecimen ID" inwhere="true" infieldlist="true" />
                 <fieldname name="BIOSPECIMEN_strLocation" dname="Location" inwhere="true" infieldlist="true" />

                <displayfield name="PATIENT_strSurname" />
                <displayfield name="PATIENT_strFirstName" />
                <displayfield name="BIOSPECIMEN_strBiospecimenID" />

	</lookup_object>
        
