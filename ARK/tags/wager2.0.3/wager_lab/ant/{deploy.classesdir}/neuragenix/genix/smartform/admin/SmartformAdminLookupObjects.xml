                <fieldname name="ORGUSER_strFirstName" dname="First Name" inwhere="true" infieldlist="true" />
                <fieldname name="ORGUSER_strLastName" dname="Last Name" inwhere="true" infieldlist="true" />   
                <fieldname name="ORGUSER_strJobTitle" dname="Job Title" inwhere="false" infieldlist="true" />   
