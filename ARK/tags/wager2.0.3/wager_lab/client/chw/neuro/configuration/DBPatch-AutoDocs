CREATE SEQUENCE public.ix_doc_template_key_seq
  INCREMENT 1
  MINVALUE 1
  MAXVALUE 2147483647
  START 1
  CACHE 1;

CREATE TABLE public.ix_document_template
(
  "DOC_TEMPLATE_KEY" int8 NOT NULL DEFAULT nextval('"ix_doc_template_key_seq"'::text),
  "EXTERNAL_NAME" varchar(100),
  "FILE_NAME" varchar(100),
  "DELETED" int4 DEFAULT 0,
  "TIMESTAMP" varchar(55) DEFAULT timeofday(),
  "TEMPLATE_HELP" text,
  "HIDDEN" int4 DEFAULT 0,
  CONSTRAINT ix_doc_template_key PRIMARY KEY ("DOC_TEMPLATE_KEY")
) WITH OIDS;

CREATE SEQUENCE public.ix_doctor_doctorkey_seq
  INCREMENT 1
  MINVALUE 1
  MAXVALUE 2147483647
  START 1
  CACHE 1;

CREATE TABLE public.ix_doctor
(
  "DOCTORKEY" int8 NOT NULL DEFAULT nextval('"ix_doctor_doctorkey_seq"'::text),
  "TITLE" varchar(50) default 'Dr.',
  "FIRSTNAME" varchar(100),
  "SURNAME" varchar(100),
  "DEPT" varchar(100),
    "HOSPITAL" varchar(100),
    "ADDRESS" varchar(100),
    "PHONE" varchar(100),
    "FAX" varchar(100),
    "EMAIL" varchar(100),
  "DELETED" int4 DEFAULT 0,
  "TIMESTAMP" varchar(55) DEFAULT timeofday(),
  CONSTRAINT ix_doctor_key PRIMARY KEY ("DOCTORKEY")
) WITH OIDS;

