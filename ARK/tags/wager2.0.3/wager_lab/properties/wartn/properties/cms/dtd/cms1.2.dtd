<!--This file contains all XML entities defined and used by elements ofColumbia's CMS and filesystem services.--><!-- cms-project is the root of a single project definition --><!ELEMENT cms-project (repository,preview?,build?,publish?,xml-doc*,xml-doctype*,resource-directory*,ignore-directory*,editor*)><!-- a project can define its own filesystem, or refer to a centrallydefined	system by its name --><!ELEMENT repository (filesystem-def)><!ELEMENT preview (filesystem-def)><!ATTLIST preview base-url	CDATA	#REQUIRED><!ELEMENT build (filesystem-def)><!ATTLIST build base-url	CDATA	#REQUIRED><!ELEMENT publish (filesystem-def)><!ATTLIST publish base-url	CDATA	#REQUIRED><!-- note that editor-factory is borrowed from the content-types sectionbelow:	editors can be defined there globally, or here for a particularproject --><!ELEMENT xml-doctype (editor*,output*)><!ATTLIST xml-doctype label CDATA #REQUIRED	definition CDATA #REQUIRED	path	CDATA	#REQUIRED	root	CDATA	#REQUIRED	template	CDATA	#IMPLIED><!-- xml-doc is ONLY used for single document instances that MUST be presentat a predefined path in the repository, NO WILDCARDS!--><!ELEMENT xml-doc (editor*,output*)><!ATTLIST xml-doc definition CDATA #REQUIRED	path	CDATA	#REQUIRED	root	CDATA	#REQUIRED><!ELEMENT resource-directory (editor*,content+)><!ATTLIST resource-directory label CDATA #REQUIRED        path	CDATA	#REQUIRED	publish		(true|false)	#REQUIRED><!ELEMENT ignore-directory EMPTY><!ATTLIST ignore-directory path	CDATA	#REQUIRED><!ELEMENT content EMPTY><!ATTLIST content type CDATA #REQUIRED><!ELEMENT output (include*,filter*,transform?)><!ATTLIST output content-type CDATA #REQUIRED  basedir CDATA "/"><!ELEMENT include EMPTY><!ATTLIST include source CDATA #REQUIRED  data (yes|no) "yes"  metadata (yes|no) "no"><!ELEMENT filter (with-param*)><!ATTLIST filter key CDATA #REQUIRED><!ELEMENT transform (with-param*,with-filedate?,with-baseurl?)><!ATTLIST transform source CDATA #REQUIRED><!ELEMENT with-param EMPTY><!ATTLIST with-param name CDATA #REQUIRED	value CDATA	#REQUIRED><!ELEMENT with-filedate EMPTY><!ATTLIST with-filedate as CDATA "filedate"><!ELEMENT with-baseurl EMPTY><!ATTLIST with-baseurl as CDATA "baseurl"><!-- filesystems is the root of the centrally managed filesystem listing--><!ELEMENT filesystems (filesystem-def+)><!ELEMENT filesystem-def (argument+)><!ATTLIST filesystem-def name CDATA #REQUIRED	type	CDATA	#REQUIRED><!ELEMENT argument EMPTY><!ATTLIST argument index CDATA "0"	prompt	CDATA	#IMPLIED	value	CDATA	#IMPLIED	type	(text|password) 	#IMPLIED><!ELEMENT filesystem-type EMPTY><!ATTLIST filesystem-type name CDATA #REQUIRED	class	CDATA	#REQUIRED><!-- file-def is the root of a single editioned file representation --><!ELEMENT file-def (rdf:RDF?,edition*,file-output*,temp-file?)><!ATTLIST file-def content-type	CDATA	#REQUIRED        lastModified  CDATA #REQUIRED	currentEdition CDATA #IMPLIED><!ELEMENT file-output (hit*)><!ATTLIST file-output build-url CDATA #REQUIRED  publish-url CDATA #REQUIRED  label CDATA #REQUIRED  output-id CDATA #REQUIRED  output-path CDATA #REQUIRED><!ELEMENT references (hit*)><!ELEMENT temp-file (rdf:RDF?)><!ATTLIST temp-file editor	CDATA	#REQUIRED	date	CDATA	#REQUIRED><!ELEMENT edition (rdf:RDF?)><!ATTLIST edition index CDATA #REQUIRED	editor	CDATA	#REQUIRED	date	CDATA	#REQUIRED><!ELEMENT property EMPTY><!ATTLIST property name CDATA #REQUIRED	value CDATA #REQUIRED><!ELEMENT comment (#PCDATA)><!-- content-types is the root of a file defining MIME types and theirmappings to file extensions and editor-factoriesThe editors specified at the top level are global defaults.--><!ELEMENT content-types (content-type*)><!ELEMENT content-type (equivalent*,file-extension+,editor*)><!ATTLIST content-type type CDATA #REQUIRED	label	CDATA	#REQUIRED	icon	CDATA	#REQUIRED><!ELEMENT equivalent EMPTY><!ATTLIST equivalent type CDATA #REQUIRED><!ELEMENT file-extension EMPTY><!ATTLIST file-extension value CDATA #REQUIRED	preferred 	(true)		#IMPLIED><!ELEMENT editor EMPTY><!ATTLIST editor key CDATA #REQUIRED><!-- content-manager is the root of the Content manager channel modeldocument --><!ELEMENT content-manager (project-listing*,cms-project?,repository-nodes,content-types,newFileOptions?,active-editor*,search-results)><!ELEMENT project-listing EMPTY><!ATTLIST project-listing title CDATA #REQUIRED	description	CDATA #REQUIRED	key	CDATA	#REQUIRED><!ELEMENT repository-nodes (node*)><!ELEMENT node (warnings,newFileOptions*,moveOptions,copyOptions,(node*|(file-def,references?)))><!ATTLIST node path CDATA #REQUIRED	name CDATA #REQUIRED    label CDATA #REQUIRED        required    (true|false) #REQUIRED        canRead     (true|false) #REQUIRED        canWrite    (true|false) #REQUIRED        canDelete   (true|false) #REQUIRED        canCreate   (true|false) #REQUIRED        canBuild    (true|false) #REQUIRED        canPublish  (true|false) #REQUIRED        canAssign   (true|false) #REQUIRED        hasOutput   (true|false) #REQUIRED        hasEditorFactory (true|false) "false"        isFile  (true|false) #REQUIRED	expanded    (true|false) "false"        showHistory (true|false) "false"        build-url CDATA #IMPLIED        publish-url CDATA #IMPLIED><!ELEMENT warnings (delete,deleteEdition,compact)><!ELEMENT delete (#PCDATA)><!ELEMENT deleteEdition (#PCDATA)><!ELEMENT compact (#PCDATA)><!ELEMENT moveOptions (moveOption*)><!ATTLIST moveOptions currentName CDATA #REQUIRED    promptString CDATA #REQUIRED><!ELEMENT moveOption EMPTY><!ATTLIST moveOption path CDATA #REQUIRED    label CDATA #REQUIRED    current (true|false) #REQUIRED><!ELEMENT copyOptions (copyOption*)><!ATTLIST copyOptions currentName CDATA #REQUIRED    promptString CDATA #REQUIRED><!ELEMENT copyOption EMPTY><!ATTLIST copyOption path CDATA #REQUIRED    label CDATA #REQUIRED    current (true|false) #REQUIRED><!ELEMENT newFileOptions (content*,newFileOption*)><!ATTLIST newFileOptions label CDATA #REQUIRED  namedFile (true|false) #REQUIRED  type  CDATA #REQUIRED  prompt CDATA #REQUIRED><!ELEMENT newFileOption EMPTY><!ATTLIST newFileOption path CDATA #REQUIRED    label CDATA #REQUIRED><!ELEMENT active-editor EMPTY><!ATTLIST active-editor name CDATA #REQUIRED  index CDATA #REQUIRED><!ELEMENT search-results (hit*)><!ATTLIST search-results field CDATA #REQUIRED  term CDATA #REQUIRED><!ELEMENT hit EMPTY><!ATTLIST hit path CDATA #REQUIRED  extension CDATA #REQUIRED  edition CDATA #REQUIRED  title   CDATA  #REQUIRED  score CDATA #REQUIRED  typeID CDATA #REQUIRED  current (true|false) #REQUIRED  hasPreview (true|false) #REQUIRED  canWrite (true|false) #REQUIRED><!-- XmlIncludeWrapper is the root of output files generated by the IncludeFilter--><!ELEMENT cms:wrapper (cms:source,cms:include*)><!ELEMENT cms:source ANY><!ATTLIST cms:source directory CDATA #REQUIRED  path      CDATA   #REQUIRED  filename  CDATA   #REQUIRED  basename  CDATA   #REQUIRED><!ELEMENT cms:include ANY><!ATTLIST cms:include directory CDATA #REQUIRED  path      CDATA   #REQUIRED  filename  CDATA   #REQUIRED  basename  CDATA   #REQUIRED  type      CDATA   #REQUIRED>