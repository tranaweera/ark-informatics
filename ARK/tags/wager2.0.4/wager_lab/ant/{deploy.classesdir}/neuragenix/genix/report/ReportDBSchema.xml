<?xml version='1.0' encoding='utf-8'?>


<dbschema type="postgres">

	<!--  Domains description  -->
        <domain iname="REPORTCATEGORY" ename="ix_report_category" sequence="ix_report_category_seq">
		<field iname="intReportCategoryKey" ename="REPORTCATKEY" lname="Category key" cname="Category key" type="integer" length="8" unique="true" required="true" />
                <field iname="intParentKey" ename="PARENTKEY" lname="Parent key" cname="Parent key" type="integer" length="8" unique="true" required="true" />
		<field iname="strReportCategoryName" ename="NAME" lname="Category name" cname="Category name" type="string" length="10000" unique="false" required="true" />
		<field iname="strReportCategoryDesc" ename="DESCRIPTION" lname="Description" cname="Description" type="string" length="10000" unique="false" required="false" />
                <field iname="intDeleted" ename="DELETED" lname="Deleted" cname="Deleted" type="integer" length="4" unique="false" required="false" />
	</domain>
        
	<domain iname="REPORT" ename="ix_report" sequence="ix_report_seq">
		<field iname="intReportKey" ename="REPORTKEY" lname="Report key" cname="Report key" type="integer" length="8" unique="true" required="true" />
                <field iname="intParentKey" ename="REPORTCATKEY" lname="Category key" cname="Category key" type="integer" length="8" unique="true" required="true" />
		<field iname="strReportName" ename="NAME" lname="Report name" cname="Report name" type="string" length="10000" unique="false" required="true" />
                <field iname="strReportQuery" ename="QUERY" lname="MDX Query" cname="Query" type="string" length="10000" unique="false" required="true" />
		<field iname="strReportDesc" ename="DESCRIPTION" lname="Description" cname="Description" type="string" length="10000" unique="false" required="false" />
		<field iname="strMDXQueryFile" ename="MDXQUERYFILE" lname="Query" cname="Query" type="string" length="10000" unique="false" required="false" />
                <field iname="intDeleted" ename="DELETED" lname="Deleted" cname="Deleted" type="integer" length="4" unique="false" required="false" />
                <field iname="strOLAPSchemaFile" ename="OLAPSCHEMAFILE" lname="OLAP Schema" cname="OLAP Schema" type="string" length="10000" unique="false" required="false" />
	</domain>

        <domain iname="REPORTPARAMETERS" ename="ix_report_parameters" sequence="ix_report_parameters_seq">
		<field iname="intReportParameterKey" ename="REPORT_PARAMETER_KEY" lname="Parameter Key" cname="Parameter Key" type="integer" length="8" unique="true" required="true" />
                <field iname="intReportKey" ename="REPORT_KEY" lname="Report Key" cname="Report Key" type="integer" length="8" unique="true" required="true" />
                <field iname="strSchemaName" ename="SCHEMANAME" lname="Schema Name" cname="Schema Name" type="string" length="50" unique="false" required="true" />
                <field iname="strName" ename="NAME" lname="Parameter Name" cname="Parameter Name" type="string" length="50" unique="true" required="true" />
                <field iname="strType" ename="TYPE" lname="Type" cname="Type" type="string" length="10000" unique="true" required="true" lov_type="REPORT_PARAM_TYPE" />
                <field iname="strOperator" ename="OPERATOR" lname="Operator" cname="Operator" type="string" length="3" unique="true" required="true" lov_type="REPORT_PARAM_OPERATOR"/>
                <field iname="strAllowOperator" ename="ALLOWOPERATOR" lname="User Specified Operator" cname="User Specified Operator" type="string" length="3" unique="false" required="true" />
                <field iname="intDeleted" ename="DELETED" lname="Deleted" cname="Deleted" type="integer" length="4" unique="false" required="false" />
        </domain>
        
        
        
        <!--  Formfields description  -->
        <formfield name="view_report_category">
                <fieldname name="REPORTCATEGORY_intReportCategoryKey" />
		<fieldname name="REPORTCATEGORY_strReportCategoryName" />
                <fieldname name="REPORTCATEGORY_strReportCategoryDesc" />
	</formfield>
        
        <formfield name="add_report_category">
		<fieldname name="REPORTCATEGORY_strReportCategoryName" />
                <fieldname name="REPORTCATEGORY_strReportCategoryDesc" />
	</formfield>
        
        <formfield name="update_report_category">
		<fieldname name="REPORTCATEGORY_strReportCategoryName" />
                <fieldname name="REPORTCATEGORY_strReportCategoryDesc" />
	</formfield>
        
        <formfield name="view_report">
                <fieldname name="REPORT_intReportKey" />
                <fieldname name="REPORT_intParentKey" />
		<fieldname name="REPORT_strReportName" />
                <fieldname name="REPORT_strReportQuery" />
                <fieldname name="REPORT_strReportDesc" />
                <fieldname name="REPORT_strMDXQueryFile" />
                <fieldname name="REPORT_strOLAPSchemaFile" />
	</formfield>

        <formfield name="add_report">
		<fieldname name="REPORT_strReportName" />
                <fieldname name="REPORT_intParentKey" />
                <fieldname name="REPORT_strReportQuery" />
                <fieldname name="REPORT_strReportDesc" />
                <fieldname name="REPORT_strMDXQueryFile" />
                <fieldname name="REPORT_strOLAPSchemaFile" />
	</formfield>

        <formfield name="update_report">
                <fieldname name="REPORT_intParentKey" />
		<fieldname name="REPORT_strReportName" />
                <fieldname name="REPORT_strReportQuery" />
                <fieldname name="REPORT_strReportDesc" />
                <fieldname name="REPORT_strMDXQueryFile" />
                <fieldname name="REPORT_strOLAPSchemaFile" />
	</formfield>
        
        <formfield name="list_report_reportcategory">
            <fieldname name="REPORTCATEGORY_intReportCategoryKey" />
            <fieldname name="REPORTCATEGORY_intParentKey" />
            <fieldname name="REPORTCATEGORY_strReportCategoryName" />
            <fieldname name="REPORTCATEGORY_intDeleted" />
        </formfield>

        <formfield name="list_report_report">
            <fieldname name="REPORT_intReportKey" />
            <fieldname name="REPORT_intParentKey" />
            <fieldname name="REPORT_strReportName" />
            <fieldname name="REPORT_intDeleted" />
        </formfield>
    
        <!-- Report Parameters -->
        
        <formfield name="add_report_parameter">
                <fieldname name="REPORTPARAMETERS_strName" />
                <fieldname name="REPORTPARAMETERS_strSchemaName" />
                <fieldname name="REPORTPARAMETERS_strType" />
                <fieldname name="REPORTPARAMETERS_strOperator" />
                <fieldname name="REPORTPARAMETERS_intReportKey" />
                <fieldname name="REPORTPARAMETERS_strAllowOperator" />
        </formfield>
        
        <formfield name="view_report_parameter">
                <fieldname name="REPORTPARAMETERS_intReportParameterKey" />
                <fieldname name="REPORTPARAMETERS_intReportKey" />
                <fieldname name="REPORTPARAMETERS_strName" />
                <fieldname name="REPORTPARAMETERS_strSchemaName" />
                <fieldname name="REPORTPARAMETERS_strType" />
                <fieldname name="REPORTPARAMETERS_strOperator" />
                <fieldname name="REPORTPARAMETERS_strAllowOperator" />
        </formfield>
        
        <formfield name="update_report_parameter">
                <fieldname name="REPORTPARAMETERS_intReportParameterKey" />
                <fieldname name="REPORTPARAMETERS_strName" />
                <fieldname name="REPORTPARAMETERS_strSchemaName" />
                <fieldname name="REPORTPARAMETERS_strType" />
                <fieldname name="REPORTPARAMETERS_strOperator" />
                <fieldname name="REPORTPARAMETERS_strAllowOperator" />
        </formfield>
        
</dbschema>