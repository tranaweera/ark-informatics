    <xsl:template name="get-previous-month">
		<xsl:param name="offset"><xsl:value-of select="offset"/></xsl:param>
		<xsl:param name="myDate"><xsl:value-of select="myDate"/></xsl:param>
          <xsl:param name="ba"><xsl:value-of select="number( substring( $myDate, 6,2 ) ) - $offset"/></xsl:param>
          <xsl:choose>
          <xsl:when test=" $ba &lt; 1">
            <xsl:variable name="newMonth"><xsl:value-of select="12 + $ba"/></xsl:variable>
            <xsl:value-of select="number( substring( $myDate, 1,4 ) ) - 1"/>-<xsl:if test="string-length( $newMonth ) = 1 ">0</xsl:if><xsl:value-of select="$newMonth"/><xsl:value-of select="substring( $myDate, 8, 14 )"/>
          </xsl:when>
          <xsl:otherwise>
            <xsl:variable name="newMonth"><xsl:value-of select="$ba"/></xsl:variable>
            <xsl:value-of select="number( substring( $myDate, 1,4 ) )"/>-<xsl:if test="string-length( $newMonth ) = 1 ">0</xsl:if><xsl:value-of select="$ba"/><xsl:value-of select="substring( $myDate, 8, 14 )"/>
          </xsl:otherwise>
          </xsl:choose>
  </xsl:template>
   
