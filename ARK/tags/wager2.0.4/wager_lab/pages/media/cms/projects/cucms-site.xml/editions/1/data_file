				<with-baseurl as="baseurl" />
			</transform>
		</output>
		<output basedir="/vcf/" content-type="text/directory">
			<filter key="vcard_vcf">
				<with-param name="fold" value="false"/>
				<with-param name="photo" value="true"/>
			</filter>
		</output>
		<output basedir="/photo/" content-type="image/jpeg">
			<filter key="vcard_photo" />
		</output>
	</xml-doctype>
	
