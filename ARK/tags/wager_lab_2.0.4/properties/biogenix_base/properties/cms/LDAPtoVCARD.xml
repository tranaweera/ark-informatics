<ldapToVCard>
	<connection>
		<url>ldap://ldap.columbia.edu:389/o=Columbia%20University,c=US</url>
		<logonid></logonid>
		<logonpassword></logonpassword>
		<searchfields>uni,cn</searchfields>
		<usercontext></usercontext>
	</connection>
	<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:vcard="http://www.ais.columbia.edu/sws/xmlns/vcard3.0#">
		<vcard:VCARD rdf:about="#template">
			<vcard:N rdf:parseType="Resource">
				<vcard:familyName>{sn}</vcard:familyName>
				<vcard:givenName>{givenname}</vcard:givenName>
				<vcard:additionalName></vcard:additionalName>
				<vcard:honorificSuffix></vcard:honorificSuffix>
				<vcard:honorificPrefix></vcard:honorificPrefix>
			</vcard:N>
			<vcard:FN rdf:parseType="Resource">
				<vcard:text>{cn}</vcard:text>
			</vcard:FN>
            <vcard:TITLE rdf:parseType="Resource">
				<vcard:text>{title}</vcard:text>
			</vcard:TITLE>
            <vcard:ORG>
				<rdf:Seq>
					<!--<rdf:li>Columbia University</rdf:li> -->
                    <rdf:li>{ou}</rdf:li>
				</rdf:Seq>
			</vcard:ORG>
			<!--<vcard:NICKNAME>
				<rdf:Bag>
					<rdf:li></rdf:li>
				</rdf:Bag>
			</vcard:NICKNAME>    -->
			<vcard:ADR rdf:parseType="Resource">
                <vcard:type>work</vcard:type>
				<vcard:type>postal</vcard:type>
				<vcard:type>parcel</vcard:type>
                <vcard:type>intl</vcard:type>
                <vcard:type>pref</vcard:type>

				<vcard:poBox></vcard:poBox>
				<vcard:extendedAddress></vcard:extendedAddress>
				<vcard:streetAddress>{address}</vcard:streetAddress>
				<vcard:locality></vcard:locality>
				<vcard:region></vcard:region>
				<vcard:postalCode></vcard:postalCode>
				<vcard:countryName></vcard:countryName>
			</vcard:ADR>
            <!--
			<vcard:LABEL rdf:parseType="Resource">
				<vcard:text></vcard:text>
			</vcard:LABEL>
            -->
			<vcard:TEL rdf:parseType="Resource">
				<vcard:type>work</vcard:type>
                <vcard:type>pref</vcard:type>
				<vcard:phoneNumber>{telephonenumber}</vcard:phoneNumber>
			</vcard:TEL>
			<vcard:TEL rdf:parseType="Resource">
				<vcard:type>fax</vcard:type>
				<vcard:phoneNumber>{fax}</vcard:phoneNumber>
			</vcard:TEL>
            <vcard:EMAIL rdf:parseType="Resource">
				<vcard:type>internet</vcard:type>
                <vcard:type>pref</vcard:type>
				<vcard:text>{mail}</vcard:text>
			</vcard:EMAIL>
            <vcard:UID rdf:parseType="Resource">
				<vcard:text>{uni}</vcard:text>
			</vcard:UID>
            <vcard:SORT-STRING rdf:parseType="Resource">
				<vcard:text>{sn}</vcard:text>
			</vcard:SORT-STRING>
		</vcard:VCARD>
	</rdf:RDF>
</ldapToVCard>
