<?xml version="1.0" encoding="UTF-8"?>
<!-- this file is used to configure CuCMS file editors.  Two
  ways are provided to configure an editor; either by specifying
 an edu.columbia.ais.portal.cms.IEditor implementation using
 an "editor" tag, or by specifying an implementation of
 edu.columbia.ais.portal.cms.IFileEditorServantFactory in
 a "factory" tag. The "key" attribute of either element
 is used to retrieve a factory using the method
 EditorFactoryLibrary.getFactory(String key) -->

<cms-editors>
    <editor key="xml" class ="edu.columbia.ais.portal.cms.editors.xml.XmlEditor"/>
    <editor key="dublincore" class="edu.columbia.ais.portal.cms.editors.dublincore.DublinCore"/>
    <editor key="text" class="edu.columbia.ais.portal.cms.editors.text.TextEditor"/>
    <editor key="upload" class="edu.columbia.ais.portal.cms.editors.upload.UploadEditor"/>
    <editor key="vcard" class="edu.columbia.ais.portal.cms.editors.vcard.VCardEditor"/>
    <editor key="buildoptions" class="edu.columbia.ais.portal.cms.editors.buildoptions.BuildOptions"/>
</cms-editors>