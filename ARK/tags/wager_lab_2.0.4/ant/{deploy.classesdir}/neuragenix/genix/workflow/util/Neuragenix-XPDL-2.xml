<?xml version="1.0"?>

<Package Id="neuragenix.genix.workflow.fillDoctorDetails" Name="FillDoctorDetails">
    <PackageHeader>
        <XPDLVersion>1.0</XPDLVersion>
        <Vendor>Neuragenix</Vendor>
		<Created>02/02/2004</Created>
        <Description>Fill Doctor Details Workflow</Description>
    </PackageHeader>

    <Participants>
        <Participant Id="User1" Name="Workflow Admin">
            <ParticipantType Type="HUMAN"/>
            <Description>None</Description>
        </Participant>

        <Participant Id="User2" Name="Agustian Agustian">
        	<ParticipantType Type="HUMAN"/>
        	<Description>Normal User</Description>
        </Participant>
    </Participants>

    <WorkflowProcesses>
        <WorkflowProcess Id="FillDoctorDetails" Name="Fill Doctor Details" InitialActivityId="addDoctor">
            <ProcessHeader>
				<Created>02/02/2004</Created>
				<Description>Fill Doctor Details</Description>
            </ProcessHeader>

			<Parameters>
				<Parameter Name="Parameter1" Type="Integer" Desc="Description 1" />
				<Parameter Name="Parameter2" Type="Integer" Desc="Description 2" />
				<Parameter Name="Parameter3" Type="Integer" Desc="Description 3" />
			</Parameters>
			
			<Triggers>
				<Trigger Id="Trigger3" Name="Trigger 3" Action="add_doctor">
					<Domain>doctor</Domain>
					<Field>dtDOB</Field>
					<Type>Date</Type>
					<Connection1></Connection1>
					<Operator1>=</Operator1>
					<Value1>01/01/2000</Value1>
					<Connection2></Connection2>
					<Operator2></Operator2>
					<Value2></Value2>
				</Trigger>
				<Trigger Id="Trigger4" Name="Trigger 4" Action="add_doctor">
					<Domain>consent</Domain>
					<Field>dtConsentedDate</Field>
					<Type>Date</Type>
					<Connection1>AND</Connection1>
					<Operator1>=</Operator1>
					<Value1>01/01/2004</Value1>
					<Connection2>OR</Connection2>
					<Operator2>=</Operator2>
					<Value2>10/10/2000</Value2>
				</Trigger>
			</Triggers>


			<Activities>
				<Activity Id="startDoctorWorkflow" Name="Start Doctor Workflow">
					<Description>start doctor workflow</Description>
					<Performer></Performer>
					<PerformerType></PerformerType>
					<Priority></Priority>
					<Params>
					</Params>
					<ExtendedAttributes>
						<ExtendedAttribute Name="strType" Value="Start"/>
						<ExtendedAttribute Name="intPosX" Value="10"/>
						<ExtendedAttribute Name="intPosY" Value="20"/>
						<ExtendedAttribute Name="intWidth" Value="30"/>
						<ExtendedAttribute Name="intHeight" Value="40"/>
						<ExtendedAttribute Name="strBorderColor" Value="Black"/>
						<ExtendedAttribute Name="strBackgroundColor" Value="White"/>
                    </ExtendedAttributes>
				</Activity>

				<Activity Id="stopDoctorWorkflow" Name="Stop Doctor Workflow">
					<Description>stop doctor workflow</Description>
					<Performer></Performer>
					<PerformerType></PerformerType>
					<Priority></Priority>
					<Params>
					</Params>
					<ExtendedAttributes>
						<ExtendedAttribute Name="strType" Value="Stop"/>
						<ExtendedAttribute Name="intPosX" Value="10"/>
						<ExtendedAttribute Name="intPosY" Value="20"/>
						<ExtendedAttribute Name="intWidth" Value="30"/>
						<ExtendedAttribute Name="intHeight" Value="40"/>
						<ExtendedAttribute Name="strBorderColor" Value="Black"/>
						<ExtendedAttribute Name="strBackgroundColor" Value="White"/>
                    </ExtendedAttributes>
				</Activity>

                <Activity Id="addDoctor" Name="Add Doctor">
					<Description>Initial activity when this workflow is initiated</Description>
					<Performer>1</Performer>
					<PerformerType>type1</PerformerType>
					<Priority>Low</Priority> 
					<Params>
						<Param Name="intData" Type="Integer" Desc="the data" />
						<Param Name="Param2" Type="Date" Desc="Description 2"/>
					</Params>
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="strReassignable" Value="y"/>
						<ExtendedAttribute Name="strSignoff" Value="n"/>
						<ExtendedAttribute Name="strInstruction" Value="Add doctor"/>
						<ExtendedAttribute Name="intFunctionKey" Value=""/>
						<ExtendedAttribute Name="strIfFail" Value=""/>
						<ExtendedAttribute Name="strRetryDelayUnit" Value=""/>
						<ExtendedAttribute Name="flRetryDelayValue" Value=""/>
						<ExtendedAttribute Name="intRetryCounter" Value=""/>
						<ExtendedAttribute Name="strIfFailAfterRetry" Value=""/>
						<ExtendedAttribute Name="intCompletedTask" Value="1"/>
						<ExtendedAttribute Name="strMultiAction" Value="yes"/>
						<ExtendedAttribute Name="strTriggerUnit" Value="Month"/> 
						<ExtendedAttribute Name="flTriggerValue" Value="10.5"/>
						<ExtendedAttribute Name="strFinishedUnit" Value="Month"/>
						<ExtendedAttribute Name="flFinishedValue" Value="10.5"/>
						<ExtendedAttribute Name="strIntervalUnit" Value="Daily"/>
						<ExtendedAttribute Name="strIntervalValue" Value="6:00PM"/>
						<ExtendedAttribute Name="strType" Value="Human"/>
						<ExtendedAttribute Name="intPosX" Value="10"/>
						<ExtendedAttribute Name="intPosY" Value="20"/>
						<ExtendedAttribute Name="intWidth" Value="30"/>
						<ExtendedAttribute Name="intHeight" Value="40"/>
						<ExtendedAttribute Name="strBorderColor" Value="Black"/>
						<ExtendedAttribute Name="strBackgroundColor" Value="White"/>
                    </ExtendedAttributes>
                </Activity>
                
                <Activity Id="addDoctorConsent" Name="Add Doctor Consent">
					<Description>Second Activity in this workflow</Description>
					<Performer>2</Performer>
					<PerformerType>type1</PerformerType>
					<Priority>Low</Priority>
					<Params>
						<Param Name="strNameChecked" Type="String" Desc="the checked name" />
						<Param Name="Param3" Type="Date" Desc="Description 3"/>
					</Params>
					<ExtendedAttributes>
						<ExtendedAttribute Name="strReassignable" Value="y"/>
						<ExtendedAttribute Name="strSignoff" Value="n"/>
						<ExtendedAttribute Name="strInstruction" Value="add doctor consent"/>
						<ExtendedAttribute Name="intFunctionKey" Value="1"/>
						<ExtendedAttribute Name="strIfFail" Value="Continue"/>
						<ExtendedAttribute Name="strRetryDelayUnit" Value="Second"/>
						<ExtendedAttribute Name="flRetryDelayValue" Value="10"/>
						<ExtendedAttribute Name="intRetryCounter" Value="3"/>
						<ExtendedAttribute Name="strIfFailAfterRetry" Value="Continue"/>
						<ExtendedAttribute Name="intCompletedTask" Value="1"/>
						<ExtendedAttribute Name="strMultiAction" Value="yes"/>
						<ExtendedAttribute Name="strTriggerUnit" Value="Month"/> 
						<ExtendedAttribute Name="flTriggerValue" Value="10.5"/> 
						<ExtendedAttribute Name="strFinishedUnit" Value="Month"/>
						<ExtendedAttribute Name="flFinishedValue" Value="10.5"/>
						<ExtendedAttribute Name="strIntervalUnit" Value="Daily"/>
						<ExtendedAttribute Name="strIntervalValue" Value="6:00PM"/>
						<ExtendedAttribute Name="strType" Value="System"/>
						<ExtendedAttribute Name="intPosX" Value="10"/>
						<ExtendedAttribute Name="intPosY" Value="20"/>
						<ExtendedAttribute Name="intWidth" Value="30"/>
						<ExtendedAttribute Name="intHeight" Value="40"/>
						<ExtendedAttribute Name="strBorderColor" Value="Black"/>
						<ExtendedAttribute Name="strBackgroundColor" Value="White"/>
                    </ExtendedAttributes>
                </Activity>
			</Activities>

			<Transitions>
				<Transition Id="trans2.0" From="startDoctorWorkflow" To="addDoctor" Name="Goto Add Doctor" Condition="" Type="Normal" XCoord="1 2 3" YCoord="1 2 3">
				</Transition>
				<Transition Id="trans2.1" From="addDoctor" To="addDoctorConsent" Name="Goto Add Doctor Consent" Condition="" Type="Normal" XCoord="1 2 3" YCoord="1 2 3">
				</Transition>
				<Transition Id="trans2.2" From="addDoctorConsent" To="stopDoctorWorkflow" Name="Stop Doctor Workflow" Condition="" Type="Normal" XCoord="1 2 3" YCoord="1 2 3">
				</Transition>
            </Transitions>

        </WorkflowProcess>
    </WorkflowProcesses>
</Package>
