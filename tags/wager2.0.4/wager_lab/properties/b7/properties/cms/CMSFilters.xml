<?xml version="1.0" encoding="UTF-8"?>
<!-- this file is used to configure CuCMS publication filters.  Two
  ways are provided to configure an editor; either by specifying
 an edu.columbia.ais.portal.cms.IEditor implementation using
 an "editor" tag, or by specifying an implementation of
 edu.columbia.ais.portal.cms.IFileEditorServantFactory in
 a "factory" tag. The "key" attribute of either element
 is used to retrieve a factory using the method
 EditorFactoryLibrary.getFactory(String key) -->

<cms-filters>
    <filter key="vcard_photo" class ="edu.columbia.ais.portal.cms.editors.vcard.VCardPhotoFilter"/>
    <filter key="vcard_vcf" class ="edu.columbia.ais.portal.cms.editors.vcard.VCardVcfFilter"/>
</cms-filters>